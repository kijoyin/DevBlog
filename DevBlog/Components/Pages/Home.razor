@page "/"

<PageTitle>Home</PageTitle>
<PostList PostSummaries="postSummaries" Columns="4"></PostList>


@code{
    private IList<PostCardModel> postSummaries = new List<PostCardModel>();
    protected override void OnInitialized()
    {
        for(var i =0;i<10;i++)
        {
            var model =new PostCardModel($"Blog no {i+1}", "Is it any good??", "Some quick example text to build on the card title and make up the bulk of the card's content.", "a/b/c"); ;
            postSummaries.Add(model);
        }
    }
}